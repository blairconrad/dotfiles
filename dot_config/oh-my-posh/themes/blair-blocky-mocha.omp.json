{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "background": "#040404",
        "foreground": "#CDD6F4",
        "Rosewater": "#F5E0DC",
        "Flamingo": "#F2CDCD",
        "Pink": "#F5C2E7",
        "Mauve": "#CBA6F7",
        "Red": "#F38BA8",
        "Maroon": "#EBA0AC",
        "Peach": "#FAB387",
        "Yellow": "#F9E2AF",
        "Green": "#A6E3A1",
        "Teal": "#94E2D5",
        "Sky": "#89DCEB",
        "Sapphire": "#74C7EC",
        "Blue": "#89B4FA",
        "Lavender": "#B4BEFE",
        "Text": "#CDD6F4",
        "Subtext 1": "#BAC2DE",
        "Subtext 0": "#A6ADC8",
        "Overlay 2": "#9399B2",
        "Overlay 1": "#7F849C",
        "Overlay 0": "#6C7086",
        "Surface 2": "#585B70",
        "Surface 1": "#45475A",
        "Surface 0": "#313244",
        "Base": "#1E1E2E",
        "Mantle": "#181825",
        "Crust": "#11111B",
        "wight": "#FFFFFF",
        "time": "p:Lavender",
        "language": "p:Blue",
        "path": "p:Green",
        "git": "p:Yellow",
        "errorcode": "p:Peach",
        "error": "p:Red",
        "closer": "p:Pink"
    },
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "p:background",
                    "background": "p:time",
                    "properties": {
                        "time_format": " 15:04:05 "
                    },
                    "style": "plain",
                    "template": "{{ .CurrentDate | date .Format }}",
                    "type": "time"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "style": "plain",
                    "template": "\u2009",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }}",
                    "type": "python"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "go"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
                    "type": "node"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "ruby"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "java"
                },
                {
                    "foreground": "p:background",
                    "background": "p:language",
                    "style": "plain",
                    "template": "\u2009",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:path",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "p:background",
                    "background": "p:git",
                    "style": "plain",
                    "template": "\u2009",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:git",
                    "properties": {
                        "branch_max_length": 30,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "ignore_status": [
                            "E:/Dev/ei3",
                            "E:/Dev/ei2",
                            "E:/Dev/ei"
                        ]
                    },
                    "style": "plain",
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
                    "type": "git"
                },
                {
                    "foreground": "p:background",
                    "background": "p:git",
                    "style": "plain",
                    "template": "\u2009",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:errorcode",
                    "style": "plain",
                    "template": "\u2009{{ if gt .Code 0 }}{{.Code}}{{ end }}\u2009",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:error",
                    "style": "plain",
                    "template": "{{ if gt .Code 0 }} λ {{ end }}",
                    "type": "text"
                },
                {
                    "foreground": "p:background",
                    "background": "p:closer",
                    "style": "plain",
                    "template": "{{ if le .Code 0 }} λ {{ end }}",
                    "type": "text"
                },
                {
                    "background": "p:background",
                    "style": "plain",
                    "template": "\u202F",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}